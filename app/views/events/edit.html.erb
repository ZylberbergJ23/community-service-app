<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Edit An Event</title>
  </head>
  <body>
    <h1>Edit An Event</h1>
    <%= form_for @event do |f| %>
      <%= f.label :title %>
      <%= f.text_field :title %><br>
      <%= f.label :description %>
      <%= f.text_field :description %><br>
      <%= f.submit %>
    <% end %>
  </body>
</html>
