<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Login</title>
  </head>
  <body>
    <div class="row">
    </div>

    <div class="row">
      <div class="col s12">
        <div class="content">
          <h3>Login</h3>
          <%= form_tag '/login' do %>
            <%= label_tag :email %>:
            <%= text_field_tag :email %><br>
            <%= label_tag :password %>:
            <%= password_field_tag :password %><br>
            <%= submit_tag "Log In" %>
          <% end %>
        </div>
      </div>
    </div>
  </body>
</html>
